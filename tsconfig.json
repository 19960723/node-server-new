{
  "compilerOptions": {
    "target": "es6",
    "useDefineForClassFields": true,
    "module": "commonjs",
    "outDir": "dist",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    // "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      // "@": ["src/*"],
      "@/*": ["./src/*"],
      "/#/*": ["./types/*"]
    }
  },
  "include": ["src/**/*.ts", "build.ts"],
  "exclude": ["src/public/"]
}
